{
  "name": "AutoTime: Fill in the timesheet",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Fill in the timesheet, with 8*5 work time and 0.5*5 mandatory lunch break.",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "src/popup.html",
    "default_title": "Fill out the timesheet!"
  },
  "permissions": [
    "activeTab",
    "https://stratustime.centralservers.com/*"
  ]
}