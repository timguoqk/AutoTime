{
  "name": "AutoTime: Fill in the timesheet",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Fill in the timesheet, with 8*5 work time and 0.5*5 mandatory lunch break.",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "AutoTime!",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "tabs",
    "https://stratustime.centralservers.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://stratustime.centralservers.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}